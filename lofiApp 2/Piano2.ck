class Piano2 extends Piano{
    fun void playPiano(int pianoB[]){
        for (0 => int i; i<pianoB.cap(); i++){
            if(pianoB[i]){
                Math.mtof(pianoB[i]) => piano[2].freq;
                Math.random2f(0.4,.7) => piano[2].noteOn;
            }
            p.sixteenthNote => now;         
        }
    }
  }
Piano2 p2;
Notes t;
while(true){
    p2.playPiano(t.pianoNotes);
    }  